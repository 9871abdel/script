okeTransparency = 0.8
button.Text = "Randomize ESP"

-- ⏱️ 3-second cooldown before re-randomize
local cooling = false
button.MouseButton1Click:Connect(function()
	if cooling then return end
	cooling = true
	button.Text = "Please wait..."
	task.wait(3)
	randomizeESP()
	button.Text = "Randomize ESP"
	cooling = false
end
